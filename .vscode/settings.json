{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "APPBAR",
    "DOMAINNAME",
    "Hiragino",
    "Hokori",
    "Initing",
    "Logining",
    "Overridable",
    "Perior",
    "Swipeable",
    "TINYINT",
    "TTFB",
    "UPYUN",
    "USERACCOUNT",
    "Yahei",
    "accepters",
    "avif",
    "browserslist",
    "camelcase",
    "capp",
    "commitlint",
    "curpage",
    "darcula",
    "docco",
    "flexbugs",
    "flowtype",
    "fontsource",
    "geekblog",
    "ghcolors",
    "hljs",
    "immer",
    "isequalwith",
    "jsdevtools",
    "jsxs",
    "middlewares",
    "miment",
    "mixins",
    "notistack",
    "nowrap",
    "objs",
    "okaidia",
    "pmmmwh",
    "precss",
    "prodesign",
    "rehype",
    "roboto",
    "scrypt",
    "svgr",
    "swiper",
    "testname",
    "tids",
    "ttsc",
    "ttypescript",
    "typeahead",
    "unmount",
    "upupming",
    "webp",
    "zerollup"
  ],
  "[less]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // JS/TS 统一使用 eslint 进行 format
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
