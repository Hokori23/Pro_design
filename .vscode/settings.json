{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "DOMAINNAME",
    "Hiragino",
    "Hokori",
    "TINYINT",
    "TTFB",
    "UPYUN",
    "USERACCOUNT",
    "Yahei",
    "accepters",
    "avif",
    "browserslist",
    "camelcase",
    "capp",
    "commitlint",
    "flexbugs",
    "flowtype",
    "fontsource",
    "immer",
    "miment",
    "objs",
    "pmmmwh",
    "precss",
    "prodesign",
    "roboto",
    "svgr",
    "testname",
    "tids",
    "ttsc",
    "ttypescript",
    "typeahead",
    "upupming",
    "webp",
    "zerollup"
  ],
  "[less]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // JS/TS 统一使用 eslint 进行 format
  "[javascript]": {
    "editor.defaultFormatter": null
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": null
  },
  "[typescript]": {
    "editor.defaultFormatter": null
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": null
  },
  "files.eol": "\n",
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
